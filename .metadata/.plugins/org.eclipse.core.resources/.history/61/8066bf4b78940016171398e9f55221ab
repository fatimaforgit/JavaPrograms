class humans
{
		
	public void printThiss()
	{
		System.out.println("human");
	}
}

class goodHumans extends human
{
	public void printThiss()
	{
		System.out.println("good human"); 
				
	}
}

class badHumans extends human
{
	public void printThiss()
	{
		System.out.println("bad human"); 
				
	}
}